[global_config]
  title_hide_sizetext = True
  title_transmit_bg_color = "#84bee4"
  handle_size = 3
  inactive_color_offset = 0.91
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
[keybindings]
  split_horiz = <Primary><Shift>h
  split_vert = <Primary><Shift>v
[profiles]
  [[default]]
    scrollbar_position = hidden
    palette = "#000000:#f92672:#6cc72c:#fe9720:#5f91ef:#9358fe:#36af90:#8c8c8c:#4c4c4c:#ff80f4:#a7e22e:#ffee99:#5fd9ef:#ae82fe:#66efd5:#c7cece"
    background_image = None
    background_darkness = 0.4
    use_system_font = False
    foreground_color = "#e7e7e7"
    icon_bell = False
    show_titlebar = False
    font = Ubuntu Mono 12
    background_color = "#2d2d2d"
    scrollback_infinite = True
  [[monokai]]
    scrollbar_position = hidden
    palette = "#000000:#f92672:#6cc72c:#fe9720:#5f91ef:#9358fe:#36af90:#8c8c8c:#4c4c4c:#ff80f4:#a7e22e:#ffee99:#5fd9ef:#ae82fe:#66efd5:#c7cece"
    background_image = None
    foreground_color = "#ffffff"
    icon_bell = False
    font = DejaVu Sans Mono 10
    background_color = "#0d0d0d"
    scrollback_infinite = True
  [[Solarized Light]]
    scrollbar_position = hidden
    palette = "#073642:#dc322f:#859900:#b58900:#268bd2:#d33682:#2aa198:#eee8d5:#002b36:#cb4b16:#586e75:#657b83:#839496:#6c71c4:#93a1a1:#fdf6e3"
    background_image = None
    background_darkness = 0.9
    use_system_font = False
    foreground_color = "#3c3c3c"
    show_titlebar = False
    font = Ubuntu Mono 12
    background_color = "#fdf6e3"
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
    [[[window0]]]
      type = Window
      parent = ""
[plugins]
